   "source": [
    "n = int(input())\n",
    "bi = bin(n)[3:]\n",
    "cnt = 0\n",
    "res = set()\n",
    "for i in list(bi):\n",
    "    if i == '0':\n",
    "        cnt += 1\n",
    "    else:\n",
    "        res.add(cnt)\n",
    "        cnt = 0\n",
    "bin(n)[2:]"
   ]
